#include <iostream>

using namespace std;


int main()
{
   int n;
   cin>>n;
   int rem=0;
   if(n>100){
     int ans100=n/100;
     rem=n-ans100*100;
     cout<<ans100<<endl;
   }
   if(rem>50){
      int ans50=rem/50;
       rem=rem-ans50*50;
       cout<<ans50<<endl;
   }
   if(rem>10){
      int ans10=rem/10;
       rem=rem-ans10*10;
       cout<<ans10<<endl;
   }
   if(rem>1){
      int ans1=rem/1;
       rem=rem-ans1*1;
       cout<<ans1<<endl;
   }
   

    return 0;
}

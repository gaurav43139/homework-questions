#include <iostream>

using namespace std;
void fib(int n){
    int a=0;
    int b=1;
    for(int i=0;i<n;i++){
        int ans=a+b;
        a=b;
        b=ans;
        cout<<ans;
    }
}

int main()
{
    int n;
    cin>>n;
    int a=0;
    int b=1;
    cout<<a<<b;
    fib(n);
    

    return 0;
}
